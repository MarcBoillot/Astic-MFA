{
  "_version": "1.1.0",
  "sap.app": {
    "_version": "1.1.0",
    "id": "wwl",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "",
    "sourceTemplate": {
      "id": "ui5template.basicSAPUI5ApplicationProject",
      "version": "1.32.0"
    }
  },
  "sap.ui": {
    "_version": "1.1.0",
    "technology": "UI5",
    "icons": {
      "favIcon": "img/logo-SWW.png"
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    },
    "supportedThemes": [
      "sap_hcb",
      "sap_bluecrystal",
      "sap_belize"
    ]
  },
  "sap.ui5": {
    "_version": "1.1.0",
    "rootView": {
      "viewName": "wwl.view.App",
      "type": "XML"

    },

    "dependencies": {
      "minUI5Version": "1.30.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.ui.layout": {},
        "sap.ui.table": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "wwl.i18n.i18n"
        }
      },
      "conf": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "wwl.conf"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "wwl.view",
        "controlId": "MasterApp",
        "controlAggregation": "pages",
        "targetAggregation": "pages"
      },
      "routes": [
        {
          "pattern": "Login",
          "name": "Login",
          "view": "Login"
        },
        {
          "pattern": "",
          "name": "Master",
          "view": "Master"
        },
        {
          "pattern": "AddOrder",
          "name": "NewOrder",
          "view": "NewOrder"
        },
        {
          "pattern": "Item",
          "name": "Item",
          "view": "Item"
        }


      ]
    },
    "config":  {
      "sample": {
        "stretch": true,
        "files": [
          "View.view.xml",
          "Controller.controller.js",
          "Menu.fragment.xml",
          "manifest.json"
        ]
      }
    }
  }
}